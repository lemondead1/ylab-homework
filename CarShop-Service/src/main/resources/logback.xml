<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.lemondead1.carshopservice" level="DEBUG"/>

  <!-->Allowing exceptions to show in the log messages.<-->
  <logger name="org.eclipse.jetty.ee10.servlet.ServletChannelState" level="DEBUG"/>

  <root level="INFO">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>